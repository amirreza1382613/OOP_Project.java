<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.project_oop.HomePageController">
   <children>
      <AnchorPane layoutX="1.0" prefHeight="600.0" prefWidth="1000.0" style="-fx-background-color: #87CEEB;">
         <children>
            <Label layoutX="-1.0" layoutY="49.0" text="--------------------HomePage--------------------">
               <font>
                  <Font size="48.0" />
               </font>
            </Label>
            <Label layoutX="14.0" layoutY="199.0" text="choose your option please:">
               <font>
                  <Font size="36.0" />
               </font>
            </Label>
            <Button fx:id="HomePageMyProfileButton" layoutX="200.0" layoutY="432.0" mnemonicParsing="false" onAction="#pushmyprofile" text="my profile">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
            <Button fx:id="HomePageSearchButton" layoutX="480.0" layoutY="432.0" mnemonicParsing="false" onAction="#pushsearch" text="search">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
            <Button fx:id="HomePageExitButton" layoutX="490.0" layoutY="520.0" mnemonicParsing="false" onAction="#pushexit" text="Exit">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
            <Button fx:id="HomePageLogoutButton" layoutX="750.0" layoutY="432.0" mnemonicParsing="false" onAction="#pushlogout" text="logout">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
         </children></AnchorPane>
   </children>
</AnchorPane>
